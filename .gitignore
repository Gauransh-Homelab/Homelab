# ignore ansible variable files
vault-vars.yml

# Ds store
~/.DS_Store
.DS_Store

  # Certificate files (using naming convention)
  *.secret.crt
  *.secret.pem
  *.secret.key
  *.secret.cert
  *.secret.pfx
  *.secret.p12
  *.secret.yml
  *.secret.yaml

  # Alternative: prefix-based naming
  secret-*.crt
  secret-*.pem
  secret-*.key
  secret-*.cert
  secret-*.pfx
  secret-*.p12
  secret-*.yml
  secret-*.yaml

  # Or both patterns combined
  *-secret.*
  secret.*
  *secret*

# Helm secret files
helm/**/*secret*.yaml
helm/**/*secret*.yml

# Claude and Serena folders
CLAUDE.md
.claude
.serena


# Sensitive iSCSI configuration data
*iscsi-lun*.txt
*iscsi-target*.txt
synoiscsi*.log
iscsi-config*

# Kubernetes secret files
kubernetes/**/*secret*.yaml
kubernetes/**/*secret*.yml

